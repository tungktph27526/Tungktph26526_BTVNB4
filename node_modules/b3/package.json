{
  "name": "b3",
  "version": "0.4.7",
  "description": "Command-line tool for front-end team of MaiHaoChe.com",
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir src && mv bin src/bin && mv lib src/lib",
    "build": "babel src -d . --presets=es2015",
    "prepublish": "npm run build",
    "postpublish": "rm -rf bin && rm -rf lib && mv src/bin bin && mv src/lib lib && rm -rf src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ourai/bumblebee.git"
  },
  "author": "Ourai Lin <ourairyu@gmail.com> (http://ourai.ws/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ourai/bumblebee/issues"
  },
  "homepage": "https://github.com/ourai/bumblebee",
  "keywords": [
    "bumblebeejs",
    "bumblebee",
    "b3",
    "cli",
    "tool"
  ],
  "engines": "node >= 0.12.0",
  "bin": "bin/cli.js",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0"
  },
  "dependencies": {
    "generator-mhc-activity": "latest",
    "inquirer": "^0.11.0",
    "lodash": "^4.2.0",
    "meow": "^3.7.0",
    "rocketz-core": "~0.4",
    "yeoman-environment": "^1.5.2"
  }
}
